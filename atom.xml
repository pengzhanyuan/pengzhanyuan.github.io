      &lt;h1 id=&quot;插入测试数据&quot;&gt;&lt;a href=&quot;#插入测试数据&quot; class=&quot;headerlink&quot; title=&quot;插入测试数据&quot;&gt;&lt;/a&gt;插入测试数据&lt;/h1&gt;&lt;figure class=&quot;highlight applescript&quot;&gt;&lt;table&gt;&lt;tr&gt;&lt;td class=&quot;gutter&quot;&gt;&lt;pre&gt;&lt;span class=&quot;line&quot;&gt;1&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;2&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;3&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;4&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;5&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;6&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;7&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;8&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;9&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;10&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;td class=&quot;code&quot;&gt;&lt;pre&gt;&lt;code class=&quot;hljs applescript&quot;&gt;create table test_jsonb(&lt;span class=&quot;hljs-built_in&quot;&gt;id&lt;/span&gt; serial primary key,msg jsonb);&lt;br&gt;insert &lt;span class=&quot;hljs-keyword&quot;&gt;into&lt;/span&gt; test_jsonb VALUES (&lt;span class=&quot;hljs-number&quot;&gt;1&lt;/span&gt;,&amp;#x27;&amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;a&amp;quot;&lt;/span&gt;:&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;foo&amp;quot;&lt;/span&gt;, &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;b&amp;quot;&lt;/span&gt;:&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;bar&amp;quot;&lt;/span&gt;&amp;#125;&amp;#x27;);&lt;br&gt;insert &lt;span class=&quot;hljs-keyword&quot;&gt;into&lt;/span&gt; test_jsonb values( &lt;span class=&quot;hljs-number&quot;&gt;2&lt;/span&gt;,&amp;#x27;&amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;RootModule&amp;quot;&lt;/span&gt;:&amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;path&amp;quot;&lt;/span&gt;:[&lt;span class=&quot;hljs-number&quot;&gt;1&lt;/span&gt;],&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;tags&amp;quot;&lt;/span&gt;:&amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;ModuleBase1&amp;quot;&lt;/span&gt;:&amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;value&amp;quot;&lt;/span&gt;:&lt;span class=&quot;hljs-number&quot;&gt;40640&lt;/span&gt;,&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;humanstring&amp;quot;&lt;/span&gt;:&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;40640&amp;quot;&lt;/span&gt;&amp;#125;&amp;#125;&amp;#125;&amp;#125;&amp;#x27;);&lt;br&gt;&lt;br&gt;my_testdb=&lt;span class=&quot;hljs-comment&quot;&gt;#  select * from test_jsonb;&lt;/span&gt;&lt;br&gt; &lt;span class=&quot;hljs-built_in&quot;&gt;id&lt;/span&gt; |                                               msg                                                &lt;br&gt;&lt;span class=&quot;hljs-comment&quot;&gt;----+--------------------------------------------------------------------------------------------------&lt;/span&gt;&lt;br&gt;  &lt;span class=&quot;hljs-number&quot;&gt;1&lt;/span&gt; | &amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;a&amp;quot;&lt;/span&gt;: &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;foo&amp;quot;&lt;/span&gt;, &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;b&amp;quot;&lt;/span&gt;: &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;bar&amp;quot;&lt;/span&gt;&amp;#125;&lt;br&gt;  &lt;span class=&quot;hljs-number&quot;&gt;2&lt;/span&gt; | &amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;RootModule&amp;quot;&lt;/span&gt;: &amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;path&amp;quot;&lt;/span&gt;: [&lt;span class=&quot;hljs-number&quot;&gt;1&lt;/span&gt;], &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;tags&amp;quot;&lt;/span&gt;: &amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;ModuleBase1&amp;quot;&lt;/span&gt;: &amp;#123;&lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;value&amp;quot;&lt;/span&gt;: &lt;span class=&quot;hljs-number&quot;&gt;40640&lt;/span&gt;, &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;humanstring&amp;quot;&lt;/span&gt;: &lt;span class=&quot;hljs-string&quot;&gt;&amp;quot;40640&amp;quot;&lt;/span&gt;&amp;#125;&amp;#125;&amp;#125;&amp;#125;&lt;br&gt;(&lt;span class=&quot;hljs-number&quot;&gt;2&lt;/span&gt; rows)&lt;br&gt;&lt;/code&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
    
